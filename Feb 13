/**
 * @param {string} s
 * @param {number} k
 * @return {string}
 */
var reverseStr = function(s, k) {
     let arr=s.split("");// strings are immutable
for(let i=0;i<arr.length;i=i+2*k){
  let left=i;
  let right=i+k-1;
  while(left<right){
      [arr[left],arr[right]]=[arr[right],arr[left]];
      left++;
      right--;
  }
  
}

return arr.join("");
};
