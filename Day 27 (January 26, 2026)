const maxMinArray=(nums)=>{
    let max=min=nums[0];
    for(let i=0;i<nums.length;i++){
        if(nums[i]>max){
            max=nums[i];
        }
        if(nums[i]<min){
            min=nums[i];
        }
    }
    return [max,min];
}

console.log(maxMinArray([1, 5, 7, 2, 9, 3]))

// revised kadanes algorithm
