// count character frequency
// Input: "aabbc"
// Output: { a: 2, b: 2, c: 1 }

function countCharFreq(str){
    const output={};
    for(let i of str){
        if(output.hasOwnProperty(i)){
            output[i]++;
        } else{
            output[i]=1;
        }
    }
    return output;
}
console.log(countCharFreq("aabbc"))

// check if a number is pallindrome

function checkPallindrome(num){
const str=num.toString();
const revStr=str.split("").reverse().join("");
console.log(str,"and",revStr);
if(str===revStr){
    return true;
} else{
    return false;
}
}
console.log(checkPallindrome(1226))

/**
 * @param {number[]} nums
 * @return {boolean}
 */
var containsDuplicate = function(nums) {
    const uniqueArr=new Set(nums);
    if(nums.length!==[...uniqueArr].length){
        return true;
    } else{
        return false;
    }
};


ðŸ§© Task: Show / Hide Text
Requirements:

Display a text: â€œHello, React!â€

A button labeled â€œToggleâ€

Clicking the button should:

Hide the text if visible

Show the text if hidden

Technical constraints:

Use functional component

Use useState

No styling

ðŸ“Œ Focus on:

Boolean state

Conditional rendering


import React, { useState } from 'react';
import './style.css';

export default function App() {
  const [displayText, setDisplayText] = useState(false);
  const handleDisplay = () => {
    setDisplayText(!displayText);
  };
  return (
    <div>
      {displayText && <h1>Hello React!</h1>}
      <button onClick={handleDisplay}>
        {displayText ? 'hide' : 'show'} Text
      </button>
    </div>
  );
}



